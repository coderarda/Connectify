<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignHeight="100"
             x:Class="Connectify.Controls.ListItem">
  <ListBoxItem Padding="4">
      <TextBlock TextAlignment="End" x:Name="textBlock" Text="{Binding Title, RelativeSource={RelativeSource AncestorType=UserControl}}" />
  </ListBoxItem>
</UserControl>
